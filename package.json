{
  "name": "poznamkyapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo export:embed --platform android --dev false --bundle-output ./temp/bundle.js --sourcemap-output ./temp/bundle.map",
    "build:android-dev": "expo export:embed --platform android --dev true --bundle-output ./temp/bundle.js --sourcemap-output ./temp/bundle.map",
    "prebuild": "expo prebuild --clean",
    "clean": "expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@firebase/app": "^0.14.2",
    "@firebase/firestore": "^4.9.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "date-fns": "^4.1.0",
    "expo": "^53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-uuid": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/metro-config": "^0.20.14",
    "@types/react": "^19.0.10",
    "@types/react-navigation": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "metro": "^0.82.3",
    "metro-core": "^0.82.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
